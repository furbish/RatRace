package logicClasses;

public class GamblingCenter {
	/**
	 * @author Jeff Furbihs 9/11/2014
	 * This class wil interact with the RaceGod's results
	 * 
	 */
		public static int bet;
		public static int resultCash;
		
		public GamblingCenter() {
		}
		
		//Methods
		public static void getBet(int x) {
			bet=x;
		}
		public static int getResultCash() {
			return resultCash;
		}


}
