/**
 * 
 */
package logicClasses;


public class RaceTrack {
	
	/**
	 * @author Jeff Furbish 9/11/2014
	 * RaceTrack will be extended by the varied types of tracks, and called upon by RaceGod. 
	 */
		public int laps;
		public String type;

		
		public RaceTrack() {
		
		}
		public int getLaps() {
			return laps;
		}
		public String getCourseType() {
			return type;
		}

}
